                                            }
                                        ],
                                        "body": {
                                            "mode": "raw",
                                            "raw": "{\n  \"frontIdCard\": \"true\",\n  \"backIdCard\": \"true\",\n  \"idFaceRecognition\": \"true\",\n  \"vaccinePassport\": \"true\",\n  \"amlo\": \"true\",\n  \"dopa\": \"true\",\n  \"claimType\": \"payer\"\n}",
